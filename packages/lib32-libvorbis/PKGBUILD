pkgname=lib32-libvorbis
pkgver=1.3.1
pkgrel=1
pkgdesc="Vorbis codec library"
url="http://www.xiph.org/ogg/vorbis/"
arch=('x86_64')
license=('custom')
groups=('lib32')
depends=('lib32-libogg')
source=(ftp://ftp.archlinux.org/extra/os/i686/${pkgname/lib32-/}-${pkgver}-${pkgrel}-i686.pkg.tar.xz)
md5sums=('d4056f4dc36d882f5dc953cb309ad68b')

build() {

mkdir -p ${pkgdir}/opt/lib32/usr/lib/
cp -R ${srcdir}/usr/lib/*.so* $pkgdir/opt/lib32/usr/lib

}

