# Contributor: William Rea <sillywilly@gmail.com>

pkgname=psyco
pkgver=1.6
pkgrel=1
pkgdesc="JIT compiler for Python"
arch=('i686')
url="http://psyco.sourceforge.net"
license="MIT"
depends=('python' 'glibc')
source=(http://downloads.sourceforge.net/sourceforge/psyco/psyco-$pkgver-src.tar.gz)
md5sums=('8816fca8ba521e05d18dde3e1a11b0bd')

build() {
    cd $startdir/src/psyco-$pkgver
    python setup.py install --root=$startdir/pkg
    install -D COPYING.txt $startdir/pkg/usr/share/licenses/$pkgname/COPYING
}
